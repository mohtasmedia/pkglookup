{
  "name": "pkg-lookup",
  "version": "1.0.4",
  "description": "Get statistics on packages",
  "author": "Tom Sturge",
  "keywords": [
    "package",
    "statistics",
    "pkg",
    "lookup",
    "npm",
    "information"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohtasmedia/pkg-lookup/issues"
  },
  "homepage": "https://github.com/mohtasmedia/pkg-lookup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohtasmedia/pkg-lookup.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "bump": "standard-version",
    "lint": "eslint . --ext .ts",
    "build": "npm-run-all --silent clean -p compile",
    "clean": "rimraf dist",
    "compile": "ncc build src/cli.ts -o dist -m",
    "release": "npm run build -s && npm run bump && git push --follow-tags origin master && npm publish",
    "test": "snyk test && jest",
    "test:coverage": "jest --coverage",
    "snyk-protect": "snyk protect"
  },
  "bin": {
    "pkg-lookup": "dist/index.js"
  },
  "files": [
    "dist/index.js"
  ],
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@zeit/ncc": "0.22.3",
    "eslint": "8.33.0",
    "jest": "29.7.0",
    "nock": "13.3.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "snyk": "1.1094.0",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.4",
    "typescript": "4.9.5"
  },
  "synk": true
}
